                new Thread(ft,"").start();  
            }  
        }  
        try  
        {  
            System.out.println("ï¼š"+ft.get());  
        } catch (InterruptedException e)  
        {  
            e.printStackTrace();  
        } catch (ExecutionException e)  
        {  
            e.printStackTrace();  
        }  
  
    }
